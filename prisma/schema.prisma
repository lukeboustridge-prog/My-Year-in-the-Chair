datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Visit {
  id        String   @id @default(cuid())
  lodgeName String
  lodgeNo   String
  date      DateTime
  notes     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Working {
  id          String   @id @default(cuid())
  lodgeName   String
  lodgeNo     String
  workingType String
  date        DateTime
  notes       String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
