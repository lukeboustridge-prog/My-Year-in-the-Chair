// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model LodgeWorking {
  id          String   @id @default(cuid())
  title       String
  date        DateTime
  lodgeName   String?
  lodgeNumber String?
  notes       String?
  createdBy   String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
